@model IEnumerable<FindMyPet.Models.Type_Animal>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CreateTypeAnimal")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.libelle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.libelle)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditTypeAnimal", new { id=item.id }) |
            @Html.ActionLink("Details", "DetailsTypeAnimal", new { id=item.id }) |
            @Html.ActionLink("Delete", "DeleteTypeAnimal", new { id=item.id })
        </td>
    </tr>
}

</table>
